                intersect: (("if (enable_table_u == 0) { return scene_intersection_none; }\n\n// thanks to @iq for https://www.shadertoy.com/view/ld23DV\n\nvec3 rad = vec3(table_h_u, table_width_u, table_w_u);\n\nvec3 rdd = r.d.xyz;\nvec3 roo = r.o.xyz;\n\n// ray-box intersection in box space\nvec3 m = 1.0/rdd;\nvec3 n = m*roo;\nvec3 k = abs(m)*rad;\n\nvec3 t1 = -n - k;\nvec3 t2 = -n + k;\n\nfloat tN = max( max( t1.x, t1.y ), t1.z );\nfloat tF = min( min( t2.x, t2.y ), t2.z );\n\nif(tN > tF || tF < 0.0) return scene_intersection_none;\n\nvec3 nor = -sign(rdd)*step(t1.yzx,t1.xyz)*step(t1.zxy,t1.xyz);\n\nreturn SceneIntersection(triangle_white_M, SurfaceIntersection(true, tN, 0., 0., nor), false);")),
