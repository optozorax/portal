        (
            name: "sphere",
            data: (("SurfaceIntersection intersect_sphere(Ray r, vec4 center, float radius) {\n  r.o -= center;\n  vec3 op = -r.o.xyz;\n  float b = dot(op, r.d.xyz);\n  float det = b*b - dot(op, op) + sqr(radius);\n  if (det < 0.) return intersection_none;\n\n  det = sqrt(det);\n  float t = b - det;\n  if (t < 0.) t = b + det;\n  if (t < 0.) return intersection_none;\n\n  vec4 pos = r.o + r.d * t;\n  vec3 n = normalize(pos.xyz);\n\n  float u = atan(pos.z, pos.x);\n  float v = atan(sqrt(pos.x * pos.x + pos.z * pos.z), pos.y);\n\n  return SurfaceIntersection(true, t, u, v, n);\n}")),
